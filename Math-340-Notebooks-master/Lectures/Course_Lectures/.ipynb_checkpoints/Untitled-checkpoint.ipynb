   "source": [
    "import numpy as np\n",
    "import scipy.integrate as spqd\n",
    "print spqd.quad(lambda x: np.cos(np.cos(x)), 0 ,np.pi/2.)"
   ]
  },
  {
   "cell_type": "code",
